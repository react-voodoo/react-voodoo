{
  "default": {
    "rootFolder": "src",
    "basedOn": "component",
    "libsPath": "./packages",
    "vars": {
      "rootAlias": "react-voodoo",
      "entryPoint": "./src",
      "production": true,
      "externals": true,
      "babelPreset": {
        "targets": {
          "browsers": "> 0.25%, not dead, safari >= 7, ios_saf >= 7, chrome >= 52"
        },
        "loose": true
      },
      "webpackPatch": {
        "optimization": {
          //          "namedModules": true
        },
        "devtool": "source-map"
      }
    },
    "extend": [
      "lpack-react"
    ]
  },
  "staging": {
    "rootFolder": "src",
    "basedOn": "component",
    "libsPath": "./packages",
    "vars": {
      "rootAlias": "react-voodoo",
      "entryPoint": "./src",
      "externals": true,
      "production": false,
      "babelPreset": {
        "targets": {
          "browsers": "> 0.25%, not dead, safari >= 7, ios_saf >= 7, chrome >= 52"
        },
        "loose": true
      },
      "webpackPatch": {
      }
    },
    "extend": [
      "lpack-react"
    ]
  },
  "samples": {
    "rootFolder": "samples",
    "basedOn": "static",
    "libsPath": "./packages",
    "vars": {
      "rootAlias": "Samples",
      "production": false,
      "targetDir": "dist.samples",
      "entryPoint": "Samples/index.js",
      "HtmlWebpackPlugin": {
        "template": "./samples/index.html.tpl"
      },
      "babelPreset": {
        "targets": {
          "browsers": "> 0.25%, not dead, safari >= 7, ios_saf >= 7, chrome >= 52"
        },
        "loose": true
      },
      "webpackPatch": {
        "resolve": {
          "alias": {
            "react-voodoo": "Samples/voodoo.js"
          }
        }
      }
    },
    "extend": [
      "lpack-react"
    ]
  },
  "samplesDev": {
    "rootFolder": "samples",
    "libsPath": "./packages",
    "basedOn": "static",
    "vars": {
      "rootAlias": "Samples",
      "devServer": true,
      "production": false,
      "entryPoint": "Samples/index.js",
      "HtmlWebpackPlugin": {
        "template": "./samples/index.html.tpl"
      },
      "babelPreset": {
        "targets": {
          "browsers": "> 0.25%, not dead, safari >= 7, ios_saf >= 7, chrome >= 52"
        },
        "loose": true
      },
      "webpackPatch": {
        "resolve": {
          "alias": {
            "react-voodoo": "Samples/voodoo.js"
          }
        }
      }
    },
    "extend": [
      "lpack-react"
    ]
  }
}